// Define o pino do LED
const int ledPin = 13;

void setup() {
  // Configura o pino do LED como sa√≠da
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Liga o LED
  digitalWrite(ledPin, HIGH);
  delay(1000); // Aguarda 1 segundo

  // Desliga o LED
  digitalWrite(ledPin, LOW);
  delay(1000); // Aguarda 1 segundo
}